# Base URL (optional). If omitted, demo base URL is used.
# TACHIBANASHI_BASE_URL=https://demo-kabuka.e-shiten.jp/e_api_v4r8/
# TACHIBANA_BASE_URL=https://demo-kabuka.e-shiten.jp/e_api_v4r8/

# Credentials (required)
TACHIBANASHI_LOGIN_ID=your_login_id
TACHIBANASHI_PASSWORD=your_password
# Alternative names (also accepted)
# TACHIBANA_USER_ID=your_login_id
# TACHIBANA_PASSWORD=your_password

# Optional settings
# TACHIBANASHI_TIMEOUT=30s
# TACHIBANASHI_USER_AGENT=tachibanashi
# TACHIBANASHI_INSECURE_TLS=false

# Price snapshot example
# TACHIBANASHI_CODES=6501,6502,6503
# TACHIBANASHI_COLUMNS=pDPP,pPRP,tDPP:T
